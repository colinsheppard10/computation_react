{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "colinsheppard10/col-client",
            "hostname": "client",
            "essential": false,
            "memory": 128
        },
        {
            "name": "api",
            "image": "colinsheppard10/col-api",
            "hostname": "api",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "colinsheppard10/col-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": [
                "client",
                "api"
            ],
            "memory": 128
        }
    ]
}